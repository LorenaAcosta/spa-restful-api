	<queryString language="SQL">
		<![CDATA[select distinct c.timbrado, c.inicio_vigencia as inicio, c.fin_vigencia as fin, v.comprobante_completo as comprobante, v.fecha, u.ruc,
	(u.nombre) as cliente, coalesce(u.direccion, '___') as direccion
from ventas v 
join usuario u on u.usuario_id = v.usuario_id
join comprobante c on c.comprobante_id = v.comprobante_id
